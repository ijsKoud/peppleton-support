// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model botBlacklist {
  id String @id
}

model ticket {
  status    String  @default("unclaimed")
  department String
  userId    String
  caseId    String  @id
  lastMsg   Int?
  channelId String?
  claimerId String?
}
