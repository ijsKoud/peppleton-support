// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model stats {
  userId String @id
  guildId String
  messages String[]
}

model ticket {
  userId String @id
  guildId String
  messages String[]
}

model botBlacklist {
  id String @id
}
